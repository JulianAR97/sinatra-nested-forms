<h1>Welcome to the Nested Forms Lab!</h1>

<form action="/pirates" method="post">
    <p>Pirate Name: <input type="text" name="pirate[name]"></p>
    <p>Height: <input type="text" name="pirate[height]"></p>
    <p>Weight: <input type="text" name="pirate[weight]"></p>
    <p>
        Ship1: Name: <input id="ship_name_1" type="text" name="pirate[ships][][name]">
        Type: <input id="ship_type_1" type="text" name="pirate[ships][][type]">
        Booty: <input id="ship_booty_1" type="text" name="pirate[ships][][booty]" id="ship_booty_1">
    </p>
    <p>
        Ship2: Name: <input id="ship_name_2" type="text" name="pirate[ships][][name]">
        Type: <input id="ship_type_2" type="text" name="pirate[ships][][type]">
        Booty: <input id="ship_booty_2" type="text" name="pirate[ships][][booty]">
    </p>
    <input type="submit" value="Submit">
</form>
